{
  "name": "authentication-gw",
  "version": "1.0.0",
  "description": "An authentication flow service",
  "scripts": {
    "start": "npm-run-all build serve",
    "build": "tsc --skipLibCheck",
    "serve": "serverless offline --stage offline",
    "watch": "tsc --skipLibCheck -w",
    "test": "jest",
    "predeploy": "./pulumi/scripts/prepare-dependencies-artifact.sh",
    "deploy": "pulumi -C ./pulumi up",
    "clean": "rimraf dist pulumi/.lambda"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Virtual-Finland-Development/authentication-gw.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Virtual-Finland-Development/authentication-gw/issues"
  },
  "homepage": "https://github.com/Virtual-Finland-Development/authentication-gw#readme",
  "dependencies": {
    "aws-sdk": "^2.1214.0",
    "aws-serverless-swagger-ui": "^1.0.1",
    "axios": "^0.27.2",
    "openapi-backend": "^5.5.0"
  },
  "devDependencies": {
    "@pulumi/aws": "^5.14.0",
    "@pulumi/pulumi": "^3.40.0",
    "@types/aws-lambda": "^8.10.102",
    "@types/node": "^18.7.15",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "serverless": "^3.22.0",
    "serverless-offline": "^10.0.0",
    "serverless-plugin-include-dependencies": "^5.0.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  }
}