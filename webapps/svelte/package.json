    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "openapi-typescript-codegen": "^0.23.0",
    "svelte": "^3.49.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.7"
  },
  "dependencies": {
    "svelte-dialogs": "^1.2.2"
  }
}
